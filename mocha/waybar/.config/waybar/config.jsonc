{
  "height": 30,
  "spacing": 1,
  "modules-left": [
    "sway/workspaces",
    "group/swayinfo",
    "group/music"
  ],
  "modules-center": ["clock"],
  "modules-right": [

    "group/stats",
    "group/interact"

  ],

  "group/music": {
    "orientation": "horizontal",
    "modules": ["custom/playerctl", "custom/playerlabel"],
    "disable_auto_hide": false
  },

  "group/swayinfo": {
    "orientation": "horizontal",
    "modules": ["sway/mode", "sway/scratchpad"],
    "disable_auto_hide": false
  },

  "group/stats": {
    "orientation": "horizontal",
    "modules": ["memory", "cpu", "temperature", "battery"]
  },

  "group/interact": {
    "orientation": "horizontal",
    "modules": [
      "idle_inhibitor",
      "backlight",
      "network",
      "bluetooth",
      "pulseaudio",
      "custom/power",
      "custom/notification"
    ]
  },

  "custom/notification": {
    "format": "{icon}",
    "format-icons": {
      "notification": "ïƒ³<span foreground='red'><sup>ï‘„</sup></span>",
      "none": "ïƒ³",
      "dnd-notification": "ï‡·<span foreground='red'><sup>ï‘„</sup></span>",
      "dnd-none": "ï‡·",
      "inhibited-notification": "ïƒ³<span foreground='red'><sup>ï‘„</sup></span>",
      "inhibited-none": "ïƒ³",
      "dnd-inhibited-notification": "ï‡·<span foreground='red'><sup>ï‘„</sup></span>",
      "dnd-inhibited-none": "ï‡·"
    },
    "tooltip": true,
    "return-type": "json",
    "exec-if": "which swaync-client",
    "exec": "swaync-client -swb",
    "on-click": "swaync-client -t -sw",
    "on-click-right": "swaync-client -d -sw",
    "escape": true,
  },

  "backlight": {
    "format": "{icon}",
    "format-icons": ["ğŸŒ‘", "ğŸŒ˜", "ğŸŒ—", "ğŸŒ–", "ğŸŒ•"],
    "tooltip-format": "Brightness: {percent}%"
  },

  "battery": {
    "interval": 60,
    "states": {
      "warning": 30,
      "critical": 15
    },
    "format": "{capacity}% {icon}",
    "format-icons": ["ï‰„", "ï‰ƒ", "ï‰‚", "ï‰", "ï‰€"],
    "max-length": 25,
    "tooltip-format": "{timeTo} ({power}W)"
  },

  "sway/workspaces": {
    "disable-scroll": false,
    "all-outputs": false,
    "warp-on-scroll": false,
    "format": "{icon}",
    "format-icons": {
      "urgent": "ïª",
      "focused": "ï†’",
      "default": "ó°§"
    }
  },

  "sway/window": {
    "format": "{title}",
    "max-length": 60,
    "all-outputs": false,
    "offscreen-css": true,
    "offscreen-css-text": "(inactive)",
    "rewrite": {
      "(.*) - Mozilla Firefox": "ï‚¬ $1",
      "(.*) - fish": "> [$1]"
    }
  },

  "bluetooth": {
    "format-on": "ïŠ”",
    "format-off": "ïŠ”",
    "format-connected": "ïŠ“",
    "format-connected-battery": "ïŠ“",
    "format-device-preference": ["ugreen_1", "ugreen_2"],
    "tooltip-format": "{controller_alias} {controller_address}\n{num_connections} connected",
    "tooltip-format-connected": "{controller_alias} {controller_address}\n{num_connections} connected\n\n{device_enumerate}",
    "tooltip-format-enumerate-connected": "{device_alias} {device_address}",
    "tooltip-format-enumerate-connected-battery": "{device_alias} {device_address} {device_battery_percentage}%",
    "on-click": "blueman-manager"
  },
  "sway/mode": {
    "format": "{}"
  },

  "custom/playerctl": {
    "format": "{icon}",
    "return-type": "json",
    "max-length": 64,
    "exec": "playerctl -a metadata --format '{\"text\": \"{{markup_escape(artist)}} - {{markup_escape(title)}}\", \"tooltip\": \"{{playerName}} ({{status}})\", \"alt\": \"{{status}}\", \"class\": \"{{status}}\"}' -F",
    "on-click": "playerctl play-pause",
    "on-click-middle": "playerctl previous",
    "on-click-right": "playerctl next",
    "format-icons": {
      "Playing": "ó°’®  ó°¥  ó°’­",
      "Paused": "ó°’®  ó°Œ  ó°’­"
    }
  },

  "custom/playerlabel": {
    "format": "<span>{}</span>",
    "return-type": "json",
    "max-length": 48,
    "exec": "playerctl -a metadata --format '{\"text\": \"{{default(markup_escape(artist), 'test')}} - {{markup_escape(title)}}\", \"tooltip\": \"{{playerName}} : {{markup_escape(artist)}} - {{markup_escape(title)}}\", \"alt\": \"{{status}}\", \"class\": \"{{status}}\"}' -F",
    "format-icons": {
      "Playing": "ó°’® ï‹ ó°’­",
      "Paused": "ó°’® ó°¥ ó°’­"
    }
  },

  "sway/scratchpad": {
    "format": "{icon} {count}",
    "show-empty": true,
    "format-icons": ["ï‹’"],
    "tooltip": true,
    "tooltip-format": "{app}: {title}"
  },

  "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": "ï®",
      "deactivated": "ï°"
    },
    "tooltip-format-activated": "Sleep Disabled",
    "tooltip-format-deactivated": "Sleep Enabled",
 },

  "clock": {
    "interval": 60,
    "format": "{:%d-%m-%Y %I:%M%p}",
    "tooltip-format": "<tt><small>{calendar}</small></tt>",
    "calendar": {
      "mode": "year",
      "mode-mon-col": 3,
      "on-scroll": 1,
      "on-click-right": "mode",
      "format": {
        "months": "<span color='#cba6f7'><b>{}</b></span>",
        "days": "<span color='#cdd6f4'><b>{}</b></span>",
        "weeks": "<span color='#94e2d5'>W{}</span>",
        "weekdays": "<span color='#f9e2af'><b>{}</b></span>",
        "today": "<span color='#f5e0dc'><b><u>{}</u></b></span>"
      }
    },
    "actions": {
      "on-click-right": "mode",
      "on-click-forward": "tz_up",
      "on-click-backward": "tz_down",
      "on-scroll-up": "shift_up",
      "on-scroll-down": "shift_down"
    }
  },
  "cpu": {
    "interval": 3,
    "format": "{usage}% ï‹›",
    "on-click": "konsole -e btop"
  },
  "memory": {
    "interval": 3,
    "format": "{}% ï”¸",
    "on-click": "konsole -e btop",
    "tooltip-format": "Used: {used:0.1f}G/{total:0.1f}G. Swap: {swapUsed:0.1f}G/{swapTotal:0.1f}G",
    "states": {
      "critical": 80
    }
  },
  "temperature": {
    "interval": 3,
    "hwmon-path": "/sys/class/hwmon/hwmon1/temp1_input",
    "critical-threshold": 90,
    "format-critical": "{temperatureC}Â°C {icon}",
    "format": "{temperatureC}Â°C {icon}",
    "format-icons": ["ï«", "ï‹‰", "ï©"],
    "on-click": "konsole -e btop",
    "tooltip-format": "Temperature: {temperatureC}Â°C"
  },

  "network": {
    "interval": 60,
    "interface-ethernet": "enp1s*",
    "interface-wifi": "wlan0",
    "format-ethernet": "ï–",
    "format-wifi": "ï‡«",
    "tooltip-format-ethernet": "{ifname}: {ipaddr}/{cidr}",
    "tooltip-format-wifi": "{ifname} ({essid} {signalStrength}%): {ipaddr}/{cidr}",
    "format-linked": "(No IP) ï–",
    "format-disconnected": "ï‡«",
    "on-click": "konsole -e nmtui"
  },
  
  "pulseaudio": {
    "format": "{icon}",
    "format-bluetooth": "{icon}ïŠ”",
    "format-bluetooth-muted": "ïš© {icon}ïŠ”",
    "format-muted": "ïš©",
    "format-source": "{volume}% ï„°",
    "format-source-muted": "ï„±",
    "format-icons": {
      "headphone": "ï€¥",
      "hands-free": "ï–",
      "headset": "ï–",
      "phone": "ï‚•",
      "portable": "ï‚•",
      "car": "ï†¹",
      "default": ["ï€¦", "ï€§", "ï€¨"]
    },
    "tooltip-format": "{desc}\n{volume}% ï€¨ {format_source}",
    "on-click": "pavucontrol-qt"
  },

  "custom/power": {
    "format": "ï€‘",
    "tooltip": true,
    "tooltip-format": "Power Options",
    "on-click": "wlogout"
  }
}
